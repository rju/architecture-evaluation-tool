<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="de.cau.cs.se.software.evaluation.views.AnalysisResultView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="de.cau.cs.se.software.evaluation.HypergraphProvider">
      <analysisJob
            class="de.cau.cs.se.software.evaluation.emf.EMFMetamodelAnalysisJobProvider">
      </analysisJob>
   </extension>
</plugin>
